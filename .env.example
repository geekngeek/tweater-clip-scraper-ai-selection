# OpenAI API
OPENAI_API_KEY=your_openai_api_key_here

# Twitter/X Credentials (optional - not needed when using proxy)
TWITTER_USERNAME=
TWITTER_PASSWORD=

# Proxy Configuration (recommended for reliable scraping without account)  
PROXY_HOST=your_proxy_host
PROXY_PORT=your_proxy_port
PROXY_USERNAME=your_proxy_username
PROXY_PASSWORD=your_proxy_password
PROXY_ENABLED=proxy_enabled_true_or_false

# Rate Limiting
MAX_CONCURRENT_DOWNLOADS=3
SCRAPER_DELAY=1.0
VISION_RATE_LIMIT=10

# Output Configuration
OUTPUT_DIR=output
CACHE_DIR=cache
DATA_DIR=data

# Logging
LOG_LEVEL=INFO
LOG_FILE=logs/tweater.log

# Video Processing
MAX_VIDEO_SIZE_MB=100
VIDEO_QUALITY=720p
FRAME_EXTRACTION_INTERVAL=5